<main class="mt-5 ms-5 mb-5">
  <div class="row gy-4">
    
<section class="row">
  {{#with navigation}}
  {{#each links}}
    <div class="col-2">
      <a href="{{href}}">{{text}}</a>
    </div>
  {{/each}}
  <div class="col-2">
    {{#with button}}
        <a href="{{href}}" class="btn btn-primary btn-sm">{{text}}</a>
        {{/with}}
    {{!-- <a href="{{button.href}}" class="btn btn-primary btn-sm">{{button.text}}</a> --}}
  </div>
  {{/with}}
</section>

<hr>
<section>
    {{#with header}}
      <h1>{{title}}</h1>
      <p>{{description}}</p>
      {{#with button}}
      <a href="{{link}}" class="btn btn-primary">{{text}}</a>
      {{/with}}
    {{/with}}
</section>
<hr>


    {{#with city}}
      <h2>{{title}}</h2>


    <section class="row gx-2 gy-3">
      {{#each cards}}
      <div class="col-5">
        <div class="card">
          {{#with picture}}
          <img class="card-img-top" src="{{src}}" alt="{{alt}}">
          {{/with}}
          <div class="card-body">
            <h5 class="card-title">{{title}}</h5>
            <p>{{description}}</p>
              {{#with button}}
              <a href="{{href}}" class="btn btn-primary btn-lg">{{text}}</a>
              {{/with}}
          </div>
        </div>
        </div>
        {{/each}}
     </section>
    {{/with}}

<hr>

<section>
  {{#with winner}}
  <h2>{{title}}</h2>
  <hr>
  <div class="col-8">
    {{#with table}}
  
  <table class="table">
    <thead>
      <tr>{{#each head}}
        <th>{{this}}</th>
        {{/each}}
      </tr>
    </thead>
  <tbody>
    {{#each user}}
    <tr>
      <th>{{place}}</th>
      <td>{{name}}</td>
      <td>{{surname}}</td>
      <td>{{nickname}}</td>
    </tr>
    {{/each}}
  </tbody>
  </table>
  {{/with}}</div>
  {{/with}}
</section>

 <hr>
 <section>
  {{#with gallery}}
  <h2>{{title}}</h2>
  <br>
  <div class="row">
  {{#each cards}}
  <div class="col-4">
  <div class="card">
    <div class="card-header">{{name}}</div>
      <ul class="list-group">
        {{#each items}}
        <li class="list-group-item"> {{this}}</li>
        {{/each}}
        </ul>
  </div>
  </div>
  {{/each}}</div>
  {{/with}}
 </section>

<hr>

<section>
  {{#with relaxion}}
  <div class="display-3">{{title}}</div>
  <br>
  <div class="row gy-4 gx-2">
  {{#each cards}}
      <div class="col-5">
        <div class="card text-bg-secondary">
          <div class="card-header">{{header}}</div>
          <div class="card-body">
            <h2 class="card-title">{{title}}</h2>
            <p>{{description}}</p>
          </div>
        </div>
      </div>
  {{/each}}
  </div>
  {{/with}}
</section>

<hr>


{{#with rest}}
<section class="row">
{{#each cards}}
      <div class="col-6">
        <div class="card text-center">
          <div class="card-header">{{header}}</div>
          <div class="card-body">
            <h6 class="card-title">{{title}}</h6>
            <p>{{description}}</p>
            <button class="btn btn-primary btn-lg">{{button}}</button>
          </div>
          <div class="card-footer">{{footer}}</div>
        </div>
      </div>
  {{/each}}
</section>
{{/with}}

<hr>

<section>
  {{#with proposition}}
  <div class="display-2">{{title}}</div>
  <br>
  <div class="row gy-4 gx-2">
  {{#each card}}
      <div class="col-5">
        <div class="card text-light text-bg-warning">
          <div class="card-header">{{header}}</div>
          <div class="card-body">
            <h2 class="card-title">{{title}}</h2>
            <p>{{description}}</p>
          </div>
        </div>
      </div>
  {{/each}}
  </div>
  {{/with}}
</section>

<hr>
<footer>
{{#with footer}}
<h5>{{title}}</h5>

<div class="row">
  <div class="col-3">
    {{#each navigation}}
    <p><a href="{{href}}" class="btn ">{{text}}</a></p>
    {{/each}}
  </div>

  <div class="col-3">
    {{#each socialMedia}}
    <p><button src="{{link}}" class="btn btn-outline-dark btn-sm">{{name}}</button></p>
    {{/each}}
  </div>

  <div class="col-3">
    {{#each partners}}
    <p><button src="{{link}}" class="btn btn-outline-info btn-sm">{{name}}</button></p>
    {{/each}}
  </div>
</div>
{{/with}}
</div>
</footer>

<br>
<br>
  





</div>
</main>